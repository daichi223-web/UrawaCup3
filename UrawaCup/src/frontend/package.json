    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "dexie": "^4.2.1",
    "idb": "^8.0.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
